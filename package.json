{
  "name": "resetsend-all",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:sdk": "pnpm --filter @resetsend/sdk dev",
    "dev:demo": "pnpm --filter @resetsend/demo dev",
    "build:sdk": "pnpm --filter @resetsend/sdk build",
    "build:demo": "pnpm --filter @resetsend/demo build",
    "build": "pnpm build:sdk && pnpm build:demo",
    "lint": "pnpm -r lint"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "alpinejs": "^3.13.5",
    "miniprogram-api-typings": "^4.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.5",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/alpinejs": "^3.13.6",
    "@types/node": "^20.11.27",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2"
  }
}
